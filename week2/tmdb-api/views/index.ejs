<% include partials/head %>

<h1>Hoi</h1>

<article id="queryResult">
    <% if (movies) { %>
      <% movies.forEach(function(movie) { %>
         <div class="searchResult">
           <a href="<%=movie.id%>">
                <h1><%=movie.title%></h1>
                <img src="<%= url + movie.poster_path %>">
            </a>
        </div>
        <% }) %>

    <% } %>
</article>

<% include partials/foot %>
