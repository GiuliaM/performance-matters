<% include partials/head %>

<h1>Hoi</h1>

<% if (movies) { %>
  <% movies.forEach(function(movie) { %>
    <a href="<%=movie.id%>">
        <article class="searchResult">
            <h1><%=movie.title%></h1>
            <figure>
                <img src="<%= url + movie.poster_path %>">
            </figure>
        </article>
    </a>
    <% }) %>

<% } %>


<% include partials/foot %>
